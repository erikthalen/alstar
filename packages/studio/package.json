{
  "name": "@alstar/studio",
  "version": "0.0.0-beta.18",
  "type": "module",
  "main": "index.ts",
  "bin": {
    "alstar": "./bin/build-ssg.ts"
  },
  "engines": {
    "node": ">=23.8"
  },
  "scripts": {
    "dev": "node --watch index.ts"
  },
  "dependencies": {
    "@alstar/db": "workspace:*",
    "@alstar/ui": "workspace:*",
    "@hono/node-server": "1.18.1",
    "@starfederation/datastar-sdk": "1.0.0-RC.1",
    "hono": "4.8.12"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-embed": "^0.5.0",
    "prettier-plugin-sql": "^0.19.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-embed",
      "prettier-plugin-sql"
    ],
    "embeddedSqlTags": [
      "sql"
    ],
    "language": "sqlite",
    "keywordCase": "lower"
  },
  "publishConfig": {
    "access": "public"
  }
}
